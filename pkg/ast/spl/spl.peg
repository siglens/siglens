        Op: "printf",
        Param: format.(*structs.StringExpr),
    }
    if rest != nil {
        restSlice := rest.([]any)
        valueList := make([]*structs.StringExpr, len(restSlice))

        for i := range restSlice {
            valueAtom := restSlice[i].([]any)
