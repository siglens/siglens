    mvColOptionArgs := &MultiValueColOptionArgs{
        argOption: "allowempty", 
        mvColExpr: &structs.MultiValueColLetRequest{
            AllowEmpty: boolVal.(bool),
        },
    }

    return mvColOptionArgs, nil
}

